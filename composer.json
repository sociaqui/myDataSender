{
    "name": "sociaqui/data-sender-bundle",
    "description": "A very simplistic Symfony 3+ Bundle that sends data from a .json file to a Google Spreadsheet",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "google/apiclient": "2.*",
        "google/apiclient-services": "^0.83"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Sociaqui\\DataSenderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sociaqui\\DataSenderBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jaros≈Çaw Sochacki",
            "email": "sociaqui@gmail.com"
        }
    ]
}
